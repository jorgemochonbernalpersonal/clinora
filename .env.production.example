APP_NAME="Clinora"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-production-domain.com

APP_LOCALE=es
APP_FALLBACK_LOCALE=es
APP_FAKER_LOCALE=es_ES

APP_MAINTENANCE_DRIVER=file
BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mariadb
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=your-database-name
DB_USERNAME=your-db-username
DB_PASSWORD=your-db-password
DB_ROOT_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=true
SESSION_PATH=/
SESSION_DOMAIN=your-production-domain.com

BROADCAST_CONNECTION=pusher
BROADCAST_DRIVER=pusher

PUSHER_APP_ID=your-pusher-app-id
PUSHER_APP_KEY=your-pusher-app-key
PUSHER_APP_SECRET=your-pusher-app-secret
PUSHER_HOST=api.pusher.com
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

CACHE_STORE=database
CACHE_PREFIX=clinora_cache
QUEUE_CONNECTION=database
FILESYSTEM_DISK=local

# Hostinger SMTP Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.hostinger.com
MAIL_PORT=465
MAIL_USERNAME=bernalmochonjorge@gmail.com
MAIL_PASSWORD=your-email-password
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS="bernalmochonjorge@gmail.com"
MAIL_FROM_NAME="${APP_NAME}"

SANCTUM_STATEFUL_DOMAINS=your-production-domain.com
SANCTUM_GUARD=web

TELESCOPE_ENABLED=false

SENTRY_LARAVEL_DSN=your-sentry-dsn
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1

VITE_APP_NAME=""

APP_TIMEZONE=Europe/Madrid

APP_CURRENCY=EUR

APP_DEFAULT_LANGUAGE=es

INVOICE_DEFAULT_TAX_RATE=21.00
INVOICE_DEFAULT_SERIES=A

TELECONSULTATION_DEFAULT_DURATION=60
TELECONSULTATION_ENABLED=true

MODULE_PSYCHOLOGY_ENABLED=true

MEMCACHED_HOST=127.0.0.1
